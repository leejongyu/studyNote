    // 알파벳 저장
		List<String> alphas = new ArrayList<>();
		
		char[] asc2 = new char[26];  // 알파벳 개수 26개
		for (int i = 0; i < asc2.length; i++) { // 배열의 크기만큼
			asc2[i] = (char) (65 + i); // ASCII Code를 char로 형변환하여 배열에 담기
		}
		
		// char 배열의 asc2의 원소들을 String 타입으로 변경하여 알파벳 리스트에 저장
		for(char x : asc2) {
			String s = String.valueOf(x);
			alphas.add(s);
		}
		
		// char 배열의 asc2의 원소들을 하나의 String 객체로 변환
		String str = String.valueOf(asc2);
		
